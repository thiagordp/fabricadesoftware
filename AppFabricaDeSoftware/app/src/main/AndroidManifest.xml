<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="br.ufsc.fabricadesoftware.appfabricadesoftware"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".principal.Principal"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>

        <activity
            android:name=".atendimento_medico.hospital.Hospital_Ararangua"
            android:label="@string/title_hospital_ararangua">

            <intent-filter>
                <action android:name="android.intent.action.HOSPITALARARANGUA"
                    />
                <category android:name="android.intent.category.OPENABLE"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
